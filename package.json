{
    "name": "react-electron-template",
    "version": "0.0.1",
    "main": "dist_electron/main.js",
    "repository": "https://github.com/vanner-young/react-electron-template.git",
    "author": "杨金玉 <vanneryoung@gmail.com>",
    "license": "MIT",
    "keywords": [
        "react",
        "mobx",
        "i18n",
        "eslint",
        "electron",
        "axios",
        "typescript"
    ],
    "scripts": {
        "dev": "mv-cli start",
        "build": "tsc -b && mv-cli build",
        "lint": "eslint src electron --ext .js,.jsx,.ts,.tsx",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css}\" \"electron/**/*.{js,ts,json}\"",
        "prepare": "husky"
    },
    "devDependencies": {
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/eslint-plugin": "^7.16.0",
        "@typescript-eslint/parser": "^7.16.0",
        "electron": "^31.2.0",
        "electron-builder": "^25.1.8",
        "electron-rebuild": "^3.2.9",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "husky": "^9.1.0",
        "lint-staged": "^15.2.7",
        "prettier": "^3.3.2",
        "vite": "^5.3.3"
    },
    "dependencies": {
        "@ant-design/x": "^1.2.0",
        "@tailwindcss/vite": "^4.1.7",
        "@vite-electron-simple/common": "^1.1.6",
        "@vite-electron-simple/core": "^1.1.5",
        "@vitejs/plugin-react": "^4.3.1",
        "ahooks": "^3.8.5",
        "antd": "^5.21.3",
        "axios": "^1.7.7",
        "esbuild": "^0.24.2",
        "events": "^3.3.0",
        "lodash": "^4.17.21",
        "mobx": "^6.13.0",
        "mobx-react-lite": "^4.0.7",
        "mv-common": "^1.1.7",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.24.1",
        "tailwindcss": "^4.1.7",
        "typescript": "^5.5.3"
    }
}
